<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="site" extends="struts-default" namespace="/crawlerManual">
		<action name="siteAction" class="crawlerExecAction" method="listAllInfo">
			<result name="listAllInfo">/crawlerManual/crawlSiteManualA.jsp</result>
			<result name="error">/crawlerManual/error.jsp</result>
		</action>

		<action name="siteId" class="crawlerExecAction" method="saveAllInfo">
			<result name="success">/crawlerManual/success.jsp</result>
			<result name="error">/crawlerManual/success.jsp</result>
		</action>
	</package>
</struts>